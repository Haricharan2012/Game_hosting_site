    let db = requestDB.result;
 
  //};
  requestDB.onsuccess = function (e) {
    let db = requestDB.result;
    let transaction = db.transaction("score", "readwrite");
   let store = transaction.objectStore("score");
    let req=objectstore.get(1);
    req.onsuccess = function (e) {    
                    document.getElementById("result").innerHTML = "score" + req.result.score + "<br/>playerr: " + req.result.player;    
                }
                }
                };    
